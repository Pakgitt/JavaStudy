package repeatEaxm;

import java.util.Iterator;
import java.util.Scanner;

public class RepeatPp13 {
	public static void main(String[] args) {
 
		// 별표 출력
		
		Scanner sc = new Scanner(System.in);
		System.out.println("정수를 입력하세요");
		String num = sc.next();
		
		for (int i = 1; i <= num; i++) {
				System.out.println(i);
			}
			
		}
		 
	}
