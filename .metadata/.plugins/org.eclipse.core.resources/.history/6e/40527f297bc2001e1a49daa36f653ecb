package VariableExam;

import java.util.Scanner;

public class Run {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("풀어볼 문제를 고르시오 : ");
		int choice = sc.nextInt();
		
		switch (choice) {
		case 1 :
			choice = 1;
			new VariableP1() = v1.method();
			break;

		default:
			break;
		}
		
		VariableP1 v1 = new VariableP1();
		v1.method1();
	
		VariableP2 v2 = new VariableP2();
		v2.method2();

		VariableP3 v3 = new VariableP3();
		v3.method3();
		
		VariableP4 v4 = new VariableP4();
		v4.method4();

	}
}

