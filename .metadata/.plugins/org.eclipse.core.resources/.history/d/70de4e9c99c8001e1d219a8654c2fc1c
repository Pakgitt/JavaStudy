package repeatEaxamhard;

import java.util.Scanner;

public class repeatPhard3 {
	public static int main(String[] args) {
		// 2부터 입력된 수 까지의 소수를 모두 출력하고 소수의 개수를 출력

		Scanner sc = new Scanner(System.in);
		System.out.println("숫자 입력");
		int num = sc.nextInt();
		int cnt = 0; // 소수의 개수
		
		
		for (int i = 2; i <= num; i++) {
			boolean isPrime = true;
			for (int j = 2; j < num; j++) {
				if (num % i == 0)
					;
				isPrime = false;
			}
			if (isPrime == true) {
				cnt += 1;
			}
		}
		System.out.println(cnt);
		return cnt;
	}
}