package repeatEaxamhard;

import java.util.Scanner;

public class repeatPhard3 {
	public static void main(String[] args) {
		// 2부터 입력된 수 까지의 소수를 모두 출력하고 소수의 개수를 출력

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int cnt = 0; // 소수의 개수

		if (num >= 2) {
			for (int i = 2; i <= num; i++) {
				boolean isPrime = true; // 현재 숫자 i가 소수인지 여부를 나타내는 변수
			}
		} else {
			System.out.println("2이상의 숫자 입력");
		}
	}
}