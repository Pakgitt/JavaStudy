package repeatEaxamhard;

import java.util.Scanner;

public class repeatPhard3 {
	public static void main(String[] args) {
		// 2부터 입력된 수 까지의 소수를 모두 출력하고 소수의 개수를 출력

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int cnt = 0; // 소수의 개수

		if (num >= 2) {
			for (int i = 2; i <= num; i++) {
				if (num % i == 0) {
					cnt++;
					System.out.println(2 + "부터" + num + "까지 소수의 개수는 " + cnt + "개 입니다");
				}
			}
		} else {
			System.out.println("2이상의 숫자 입력");
		}
	}
}