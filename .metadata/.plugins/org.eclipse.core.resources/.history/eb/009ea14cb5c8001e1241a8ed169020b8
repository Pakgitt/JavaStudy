package fieldExam;

public class Student {
	private int grade;
	private int classroom;
	private String name;
	private double height;
	private char gender;
	{
		grade = 10;	// 초기화 블럭 보통 생성자와 필드 사이에 입력
		classroom = 3;
		name = "박세혁";
		height = 175.8;
		gender = 'M';
	}
	public Student() {
		
	}

	public void information() {
		System.out.println(grade + classroom + name + height + gender);
	}
}
