package operatroExam;

import java.util.Scanner;

public class Run {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.println("풀어볼 문제를 고르시오 :");
		int choice = sc.nextInt();

		switch (choice) {
		case 1:
			new OperatorP1().method();
			break;
		case 2:
			new OperatorP2().method();
			break;
		case 3:
			new OperatorP3().method();
			break;
		case 4:
			new OperatorP4().method();
			break;
		case 5:
			new OperatorP5().method();
			break;
		case 6:
			new OperatorP6().method();
			break;
		case 7:
			new OperatorP7().method();
			break;

		default:
			break;
		}

//		new OperatorP1().method();

//		new OperatorP2().method();

//		new OperatorP3().method();

//		new OperatorP4().method();

//		new OperatorP5().method();

//		new OperatorP6().method();

//		new OperatorP7().method();

	}

}
